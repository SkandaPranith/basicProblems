#include <iostream>
#include <string>
using namespace std;

int isPalindrome(string text) {
	int j=text.length();
	char a[j];
	for(int i=0; i<text.length(); i++) {
		a[j--]=text[i];
	}
	
	for(int i=0; i<text.length(); i++) {
		if(a[i]!=text[i]) {
			return 0;
		}
	}
	return 1;
}

int main () {
	string text;
	getline(cin,text);
	if(isPalindrome(text)) {
		cout << "Palindrome";
		return 0;
	}
	
	cout << "Not Palindrome";
}
