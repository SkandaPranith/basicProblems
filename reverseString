#include <iostream>
#include <string>
using namespace std;

void reverse(string text) {
	int j=text.length();
	char a[j];
	for(int i=0; i<text.length(); i++) {
		a[j--]=text[i];
	}
	
	for(int i=0; i<text.length(); i++) {
		cout << a[i];
	}
}

int main () {
	string text;
	getline(cin,text);
	reverse(text);
}
